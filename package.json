{
  "name": "chrome-extension-boilerplate-react-vite",
  "version": "0.5.0",
  "description": "chrome extension boilerplate",
  "license": "MIT",
  "private": true,
  "sideEffects": false,
  "repository": {
    "type": "git",
    "url": "https://github.com/Jonghakseo/chrome-extension-boilerplate-react-vite.git"
  },
  "type": "module",
  "scripts": {
    "clean:bundle": "rimraf dist && turbo clean:bundle",
    "clean:node_modules": "pnpx rimraf node_modules && pnpx turbo clean:node_modules",
    "clean:turbo": "rimraf .turbo && turbo clean:turbo",
    "clean:dist": "turbo clean:dist",
    "clean:cache": "turbo clean:cache",
    "clean": "pnpm clean:bundle && pnpm clean:turbo && pnpm clean:node_modules",
    "clean:install": "pnpm clean:node_modules && pnpm install --frozen-lockfile",
    "type-check": "turbo type-check",
    "types:build": "turbo types:build",
    "types:validate": "turbo types:validate",
    "base-build": "pnpm clean:bundle && turbo build",
    "build": "pnpm set-global-env && pnpm base-build",
    "build:chrome": "cross-env TARGET_BROWSER=chrome pnpm set-global-env && turbo build:chrome",
    "build:firefox": "cross-env TARGET_BROWSER=firefox pnpm set-global-env CLI_CEB_FIREFOX=true && turbo build:firefox",
    "build:edge": "cross-env TARGET_BROWSER=edge pnpm set-global-env && turbo build:edge",
    "build:all-browsers": "turbo build:all-browsers",
    "build:production": "cross-env NODE_ENV=production pnpm build:all-browsers",
    "base-dev": "pnpm clean:bundle && turbo ready && turbo watch dev",
    "dev": "pnpm set-global-env CLI_CEB_DEV=true && pnpm base-dev",
    "dev:chrome": "cross-env TARGET_BROWSER=chrome pnpm set-global-env CLI_CEB_DEV=true && pnpm base-dev",
    "dev:firefox": "cross-env TARGET_BROWSER=firefox pnpm set-global-env CLI_CEB_DEV=true CLI_CEB_FIREFOX=true && pnpm base-dev",
    "dev:edge": "cross-env TARGET_BROWSER=edge pnpm set-global-env CLI_CEB_DEV=true && pnpm base-dev",
    "meeting:dev": "turbo meeting:dev",
    "meeting:build": "turbo meeting:build",
    "meeting:test": "turbo meeting:test",
    "storage:build": "turbo storage:build",
    "storage:test": "turbo storage:test",
    "manifest:generate": "turbo manifest:generate",
    "manifest:validate": "turbo manifest:validate",
    "zip": "pnpm build && pnpm -F zipper zip",
    "zip:chrome": "pnpm build:chrome && pnpm -F zipper zip",
    "zip:firefox": "pnpm build:firefox && pnpm -F zipper zip",
    "zip:edge": "pnpm build:edge && pnpm -F zipper zip",
    "zip:all": "pnpm build:all-browsers && pnpm -F zipper zip",
    "package": "turbo package",
    "package:chrome": "turbo package:chrome",
    "package:firefox": "turbo package:firefox",
    "package:edge": "turbo package:edge",
    "package:all": "pnpm package:chrome && pnpm package:firefox && pnpm package:edge",
    "e2e": "pnpm zip && turbo e2e",
    "e2e:chrome": "pnpm zip:chrome && turbo e2e:chrome",
    "e2e:firefox": "pnpm zip:firefox && turbo e2e:firefox",
    "e2e:edge": "pnpm zip:edge && turbo e2e:edge",
    "e2e:all": "pnpm zip:all && turbo e2e:chrome && turbo e2e:firefox && turbo e2e:edge",
    "test": "turbo test",
    "test:unit": "turbo test:unit",
    "test:integration": "turbo test:integration",
    "test:watch": "turbo test:watch",
    "test:coverage": "turbo test --coverage",
    "lint": "turbo lint --continue",
    "lint:fix": "turbo lint:fix --continue",
    "format": "turbo format --continue -- --cache --cache-location node_modules/.cache/.prettiercache",
    "format:check": "turbo format:check --continue",
    "preview": "turbo preview",
    "preview:chrome": "cross-env TARGET_BROWSER=chrome turbo preview",
    "preview:firefox": "cross-env TARGET_BROWSER=firefox turbo preview",
    "preview:edge": "cross-env TARGET_BROWSER=edge turbo preview",
    "ci:build": "pnpm build:production",
    "ci:test": "pnpm test:unit && pnpm test:integration",
    "ci:lint": "pnpm lint && pnpm format:check && pnpm type-check",
    "ci:e2e": "pnpm e2e:all",
    "ci:package": "pnpm package:all",
    "release:prepare": "pnpm ci:build && pnpm ci:test && pnpm ci:lint",
    "release:chrome": "pnpm release:prepare && pnpm package:chrome",
    "release:firefox": "pnpm release:prepare && pnpm package:firefox",
    "release:edge": "pnpm release:prepare && pnpm package:edge",
    "release:all": "pnpm release:prepare && pnpm package:all",
    "prepare": "husky",
    "update-version": "bash bash-scripts/update_version.sh",
    "copy-env": "bash bash-scripts/copy_env.sh",
    "set-global-env": "bash bash-scripts/set_global_env.sh",
    "module-manager": "pnpm -F module-manager start",
    "postinstall": "pnpm copy-env"
  },
  "dependencies": {
    "react": "^19.1.1",
    "react-dom": "^19.1.1"
  },
  "devDependencies": {
    "@eslint/compat": "^1.3.2",
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.33.0",
    "@types/chrome": "^0.1.3",
    "@types/eslint-plugin-jsx-a11y": "^6.10.0",
    "@types/node": "^24.2.1",
    "@types/react": "^19.1.10",
    "@types/react-dom": "^19.1.7",
    "@typescript-eslint/parser": "^8.39.1",
    "autoprefixer": "^10.4.21",
    "cross-env": "^10.0.0",
    "deepmerge": "^4.3.1",
    "eslint": "^9.33.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-import-x": "^4.16.1",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-tailwindcss": "^3.18.2",
    "fast-glob": "^3.3.3",
    "fflate": "^0.8.2",
    "globals": "^16.3.0",
    "husky": "^9.1.7",
    "jiti": "^2.5.1",
    "lint-staged": "^16.1.5",
    "postcss": "^8.5.6",
    "postcss-load-config": "^6.0.1",
    "prettier": "^3.6.2",
    "prettier-plugin-tailwindcss": "^0.6.14",
    "rimraf": "^6.0.1",
    "run-script-os": "^1.1.6",
    "tailwindcss": "^3.4.17",
    "tslib": "^2.8.1",
    "tsx": "^4.20.4",
    "turbo": "^2.5.5",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.39.1",
    "vite": "^6.3.5",
    "vite-plugin-node-polyfills": "^0.24.0"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,json}": [
      "prettier --write",
      "eslint --fix"
    ]
  },
  "packageManager": "pnpm@10.11.0",
  "engines": {
    "node": ">=22.15.1"
  }
}
